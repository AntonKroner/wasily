{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "sturdy-spoon-robot",
	"main": "./index.ts",
	"compatibility_date": "2025-11-26",
	"minify": true,
	"tsconfig": "./tsconfig.json",
	"upload_source_maps": true,
	"workers_dev": true,
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"compatibility_flags": [
		"nodejs_compat",
		"enable_nodejs_wasi_module",
	],
	"build": {
		"command": "npm run transpile"
	},
	"dev": {
		"host": "example.com",
		"port": 8787
	},
	"assets": {
		"binding": "ASSETS",
		"directory": "./assets",
		"not_found_handling": "single-page-application",
		"run_worker_first": [
			"/api*"
		]
	},
	"vars": {},
	"version_metadata": {
		"binding": "version"
	},
	"kv_namespaces": [
		{
			"binding": "memoryStore",
			"id": "aaaaa",
			"preview_id": "aaaaa"
		}
	]
}
